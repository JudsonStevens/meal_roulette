p id="notice" 
  = notice

h1 Tags

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= tag.type %></td>
        <td><%= link_to 'Show', admin_tag_path(tag) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

